{
  "name": "once",
  "version": "0.0.1",
  "description": "See it once",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc --skipLibCheck",
    "watch": "tsc --skipLibCheck -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "npm run build && electron ./dist/main.js",
    "re": "nodemon --watch * --exec \"node_modules/.bin/electron .\""
  },
  "author": "",
  "devDependencies": {
    "@types/pouchdb": "^6.4.0",
    "electron": "^10.1.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.18.2",
    "cross-fetch": "^3.0.6",
    "electron-packager": "^15.1.0",
    "electron-reload": "^1.5.0",
    "on-change": "^2.1.2",
    "pouchdb": "^7.2.2"
  }
}